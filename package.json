{
  "name": "sk-mono",
  "version": "0.0.3",
  "author": "Will 'Willster' Johnson",
  "description": "SvelteKit MonoRepos Made Easy",
  "license": "GPL-3.0",
  "type": "module",
  "bin": {
    "sk-mono": "./sk-mono",
    "skmono": "./sk-mono",
    "skm": "./sk-mono"
  },
  "main": "scripts/lib.js",
  "types": "scripts/lib.d.ts",
  "scripts": {
    "preinstall": "NODE_NO_WARNINGS=1 node scripts/install.js --experimental-fetch"
  },
  "engines": {
    "": "really >=18, but fetch experimental is available in 17",
    "node": ">=17"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "mono",
    "monorepo",
    "cli",
    "sk",
    "sk-mono",
    "skmono"
  ],
  "os": [
    "linux",
    "win32",
    "win64",
    "!darwin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillsterJohnson/sk-mono.git"
  },
  "bugs": {
    "url": "https://github.com/WillsterJohnson/sk-mono/issues"
  },
  "homepage": "https://github.com/WillsterJohnson/sk-mono#readme",
  "dependencies": {
    "ffi-napi": "^4.0.3",
    "tar": "^6.1.13"
  },
  "devDependencies": {
    "@types/ffi-napi": "^4.0.7",
    "@types/node": "17"
  }
}
